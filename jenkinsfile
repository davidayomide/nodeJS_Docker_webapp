pipeline {
    agent any
    stages {
        stage('Install docker Engine') {
            steps {
                sh 'sudo apt update && sudo apt install docker.io -y &&   sudo snap install docker'
            }
        }
        stage('Build Docker File') {
            steps {
                sh 'docker build . -t ayomide_test'
                sh 'docker run -d -p 81:7000 ayomide_test'
            }
        }
    }
}
